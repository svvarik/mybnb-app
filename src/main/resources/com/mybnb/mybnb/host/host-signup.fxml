<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mybnb.mybnb.host.HostSignup">
    <children>
        <TextField fx:id="firstName" layoutX="85.0" layoutY="66.0" promptText="First Name">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></TextField>
        <TextField fx:id="lastName" layoutX="262.0" layoutY="66.0" promptText="Last Name">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></TextField>
        <TextField fx:id="sinNumber" layoutX="85.0" layoutY="110.0" onAction="#handleSinNumberTextField" promptText="SIN #">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></TextField>
        <TextField fx:id="addressLineOne" layoutX="85.0" layoutY="152.0" prefHeight="26.0" prefWidth="341.0" promptText="Address Line 1 (For eg, 22 Sussex Drive)">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></TextField>
        <TextField fx:id="addressLineTwo" layoutX="85.0" layoutY="196.0" prefHeight="26.0" prefWidth="341.0" promptText="Address Line 2 (For eg, Apt 202)">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></TextField>
        <TextField fx:id="city" layoutX="85.0" layoutY="286.0" prefHeight="26.0" prefWidth="111.0" promptText="City">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></TextField>
        <TextField fx:id="postalCode" layoutX="85.0" layoutY="242.0" prefHeight="26.0" prefWidth="111.0" promptText="Postal Code">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></TextField>
        <TextField fx:id="state" layoutX="219.0" layoutY="286.0" prefHeight="26.0" prefWidth="111.0" promptText="State">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></TextField>
        <TextField fx:id="country" layoutX="350.0" layoutY="286.0" prefHeight="26.0" prefWidth="111.0" promptText="Country">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></TextField>
        <TextField fx:id="occupation" layoutX="85.0" layoutY="327.0" prefHeight="0.0" prefWidth="111.0" promptText="Occupation">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></TextField>
        <Label layoutX="87.0" layoutY="33.0" text="Signup to be a renter!">
            <font>
                <Font name="Helvetica Neue" size="16.0" />
            </font></Label>
        <Button fx:id="submitBtn" layoutX="262.0" layoutY="353.0" mnemonicParsing="false" onAction="#handleSubmitBtnClick" text="Submit">
            <font>
                <Font name="Helvetica Neue" size="13.0" />
            </font></Button>
    </children>
</AnchorPane>
