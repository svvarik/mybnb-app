<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mybnb.mybnb.host.HostManageAvailability">
   <children>
      <TableView layoutX="36.0" layoutY="77.0" prefHeight="202.0" prefWidth="529.0">
        <columns>
          <TableColumn fx:id="dateCol" editable="false" prefWidth="208.0" sortable="false" text="Date" />
          <TableColumn fx:id="price" editable="false" minWidth="0.0" prefWidth="119.0" sortable="false" text="Price" />
            <TableColumn fx:id="availability" editable="false" minWidth="0.0" prefWidth="201.0" sortable="false" text="Availability" />
        </columns>
      </TableView>
      <Label layoutX="262.0" layoutY="26.0" text="Availability">
         <font>
            <Font name="Helvetica Neue" size="16.0" />
         </font>
      </Label>
      <Button fx:id="addDatesBtn" layoutX="368.0" layoutY="319.0" mnemonicParsing="false" onAction="#handleAddDatesBtnClick" text="Add Dates" />
      <Button fx:id="removeDates" layoutX="468.0" layoutY="319.0" mnemonicParsing="false" onAction="#handleRemoveDatesBtnClick" text="Remove Dates" />
      <Button fx:id="toggleAvailabilityBtn" layoutX="235.0" layoutY="319.0" mnemonicParsing="false" onAction="#toggleAvailabilityBtnClick" text="Toggle Availability" />
      <Button fx:id="backBtn" layoutX="36.0" layoutY="24.0" mnemonicParsing="false" onAction="#handleBackBtnClick" text="Back" />
      <Button fx:id="editPriceBtn" layoutX="147.0" layoutY="319.0" mnemonicParsing="false" onAction="#handleEditPriceBtnClick" text="Edit Price" />
   </children>
</AnchorPane>
