<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mybnb.mybnb.host.HostEditListingPrice">
   <children>
      <Label layoutX="32.0" layoutY="28.0" prefHeight="51.0" prefWidth="136.0" text="Set New Price for Selected Dates" wrapText="true">
         <font>
            <Font name="Helvetica Neue" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="price" layoutX="20.0" layoutY="100.0" onAction="#handlePriceTextField" promptText="Price" />
      <Button fx:id="submitBtn" layoutX="111.0" layoutY="137.0" mnemonicParsing="false" onAction="#handleSubmitBtnClick" prefHeight="26.0" prefWidth="69.0" text="Submit" />
      <Button fx:id="cancelBtn" layoutX="20.0" layoutY="137.0" mnemonicParsing="false" onAction="#handleCancelBtnClick" prefHeight="26.0" prefWidth="69.0" text="Cancel" />
   </children>
</AnchorPane>
